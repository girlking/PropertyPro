{
  "name": "PropertyPro",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "babel-node server/app.js",
    "bootcamp:build": "babel server/app.js -d build && build/server/app.js",
    "ayo": "nodemon example.js --exec babel-node --presets babel-preset-env ",
    "dev": "nodemon server/app.js --exec babel-node --presets babel-preset-env",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha ./server/test/**/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/girlking/PropertyPro.git"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Aprekuma Tamunoibi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/girlking/PropertyPro/issues"
  },
  "homepage": "https://github.com/girlking/PropertyPro#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "mailgun-js": "^0.22.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.0",
    "swagger-jsdoc": "^3.2.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1"
  },
  "nyc": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "plugins": [
      "istanbul"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
